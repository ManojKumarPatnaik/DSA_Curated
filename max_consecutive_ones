class Solution{
    public int maxConsecutiveOnes(int[] nums){
        int count = 0;
        int max = Integer.MIN_VALUE;

        for(int i=0; i<nums.length; i++){
            if(i == 1){
                count++;
            } else {
                count = 0;
            }
            max = Math.max(max, count);
        }
        return max;
    }
}