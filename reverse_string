class Reverse
{
    // Complete the function
    // str: input string
    public static String reverseWord(String str)
    {
        // Reverse the string str
        String[] arrOfStr = str.split("");
        String ans = "";
        for (int i = arrOfStr.length-1; i >=0; i--){
            ans += arrOfStr[i];
        }
        return ans;
    }
}
